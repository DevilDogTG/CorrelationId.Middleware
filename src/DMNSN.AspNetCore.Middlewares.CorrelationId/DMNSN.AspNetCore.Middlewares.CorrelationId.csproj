<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net8.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<Authors>DevilDogTG</Authors>
		<Company>Dimension</Company>
		<Description>Correlation ID Middleware for ASP.NET Core</Description>
		<Copyright>OneWeb.Co., Ltd.</Copyright>
		<PackageIcon>dimension.png</PackageIcon>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<PackageTags>dmnsn</PackageTags>
		<Product>Correlation ID Middleware for ASP.NET Core</Product>
		<PackageLicenseFile>LICENSE</PackageLicenseFile>
		<Version>0.0.0</Version>
	</PropertyGroup>

	<ItemGroup>
		<FrameworkReference Include="Microsoft.AspNetCore.App" />
		<None Include="..\..\LICENSE">
		  <Pack>True</Pack>
		  <PackagePath>\</PackagePath>
		</None>
		<None Include="..\..\README.md">
		  <Pack>True</Pack>
		  <PackagePath>\</PackagePath>
		</None>
		<None Include="dimension.png">
		  <Pack>True</Pack>
		  <PackagePath>\</PackagePath>
		</None>
		<PackageReference Include="Microsoft.Extensions.Options" Version="8.0.2" />
	</ItemGroup>

</Project>
